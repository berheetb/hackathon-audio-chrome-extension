{
  "manifest_version": 3,
  "name": "Saatva Audio Visualizer",
  "version": "1.0",
  "permissions": [
    "tabs",
    "activeTab",
    "tabCapture",
    "audio",
    "scripting"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "index.html"
  },
  "host_permissions": [
    "<all_urls>"
  ]
}
